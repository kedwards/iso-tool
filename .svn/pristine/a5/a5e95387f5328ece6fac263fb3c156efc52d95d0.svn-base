{
	"name": "kedwards/ieso-tool",
	"description": "Tool to query and download IESO data",
	"type": "project",
	"keywords": ["IESO", "Consulting", "Project"],
	"homepage": "https://github.com/kedwards/ieso-tool",
	"license": "Proprietary",
	"authors": [
		{
			"name": "Kevin Edwards",
			"email": "kedwards@kevinedwards.ca",
			"homepage": "https://kevinedwards.ca",
			"role": "Developer"
		}
	],
    "autoload": {
		"psr-4": {
			"LivITy\\": "src/LivITy/src/"
		}
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://wpackagist.org"
		}
	],
	"minimum-stability": "dev",
	"prefer-stable": true,
	"require": {
		"php": ">=5.3.0|7.*",
		"vlucas/phpdotenv": "^2.4",
		"oscarotero/env": "^1.1",
		"monolog/monolog": "^1.23",
		"guzzlehttp/guzzle": "^6.3",
		"box/spout": "^2.7",
		"clue/arguments": "^2.0",
		"mustache/mustache": "~2.5"
	},
    "require-dev": {
        "phpunit/phpunit": "^6.2",
        "kint-php/kint": "^2.1",
		"mikey179/vfsStream": "^1.6"
    }
}
